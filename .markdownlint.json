  "no-inline-html": {
    "allowed_elements": [
      "link",
      "meta",
      "div",
      "img",
      "a",
      "p",
      "ol",
      "ul",
      "li",
      "i",
      "b",
      "span",
      "h1",
      "h2",
      "h3",
      "h4",
      "h5",
      "h6"
